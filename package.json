{
  "name": "vite-vue-antd-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:ghpages": "vue-tsc --noEmit --skipLibCheck && vite build --mode=ghpages",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.1.13",
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.25.0",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@vitejs/plugin-vue": "^2.2.0",
    "dotenv": "^16.0.0",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.0",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-windicss": "^1.7.0",
    "vue-tsc": "^0.29.8"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design-vue/pro-layout",
        "@ant-design/icons-vue",
        "ant-design-vue",
        "ant-design-vue/es",
        "ant-design-vue/es/avatar/style/css",
        "ant-design-vue/es/button/style/css",
        "ant-design-vue/es/config-provider/style/css",
        "ant-design-vue/es/descriptions/style/css",
        "ant-design-vue/es/divider/style/css",
        "ant-design-vue/es/dropdown/style/css",
        "ant-design-vue/es/menu/style/css",
        "ant-design-vue/es/message/style/css",
        "ant-design-vue/es/pagination/style/css",
        "ant-design-vue/es/result/style/css",
        "ant-design-vue/es/space/style/css",
        "ant-design-vue/es/statistic/style/css",
        "ant-design-vue/es/tag/style/css",
        "vue",
        "vue-router"
      ]
    }
  }
}
